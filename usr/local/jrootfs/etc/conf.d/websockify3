
# webscokify container config
webport=6903
rfbport=5903

# container name
command_args="$command_args -n webscokify3"

# container config
command_args="$command_args -f /usr/local/modules/common/lxc/config/x11vnc.conf"

# container command
command_args="$command_args -- webscokify "

# container command args raw fb
command_args="$command_args *:${webport} *:${rfbport}"
