# file system       mount               type    options             dump        pass
proc                /proc               proc    defaults            0           0
/dev                /dev                none    rw,bind             0           0
devpts              /dev/pts            devpts  gid=5,mode=620      0           0
sysfs               /sys                sysfs   defaults            0           0
usbfs               /proc/bus/usb       usbfs   defaults            0           0
/tmp/jtmp           /tmp                none    rw,bind             0           0
/var/jvar           /var                none    rw,bind             0           0
/home/jhome         /home               none    rw,bind             0           0
/tmp/jtmp/run       /run                none    rw,bind             0           0

# file
/usr/etc/hosts      /etc/hosts          none    ro,bind             0           0
/etc/localtime      /etc/localtime      none    ro,bind             0           0
/etc/resolv.conf    /etc/resolv.conf    none    ro,bind             0           0
/etc/nsswitch.conf  /etc/nsswitch.conf  none    ro,bind             0           0
/etc/passwd         /etc/passwd         none    ro,bind             0           0
/etc/shadow         /etc/shadow         none    ro,bind             0           0
